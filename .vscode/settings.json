{
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.detectIndentation": true,
  "editor.tabSize": 4,
  "[markdown]": {
    "editor.detectIndentation": false
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.markdownlint": true
  },
  "cSpell.words": [
    "mdxjs",
    "mstroppel",
    "rehype"
  ]
}
